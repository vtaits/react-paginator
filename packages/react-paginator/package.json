{
  "name": "@vtaits/react-paginator",
  "version": "1.0.1",
  "description": "Simple customizable pagination component for react applications",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typings": "./dist/index.d.ts",
  "keywords": [
    "react",
    "ui",
    "pagination",
    "paginator"
  ],
  "repository": "git@github.com:vtaits/react-paginator.git",
  "author": "Vadim Taits",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --sourcemap --format esm,cjs --dts --legacy-output",
    "test:ts": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --ignore-path ../../.gitignore",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "prepare": "yarn clean && yarn build"
  },
  "peerDependencies": {
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "jest": "^29.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
